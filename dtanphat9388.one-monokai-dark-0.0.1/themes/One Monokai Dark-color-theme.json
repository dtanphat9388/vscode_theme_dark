    {
      "name": "--11b9ac",
      "scope": [
        // User-defined constant
        "constant.character",
        "constant.other",
        // Storage
        "storage",
        "keyword.control",
        // Tag Class and ID
        "entity.name.tag.class",
        "entity.name.tag.id",
        // Library constant
        "support.constant",
        // StyleSheet Variable
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass",
        // StyleSheet Function
        "function.css",
        "function.scss",
        "function.less",
        "function.sass",
        // JSON String
        "structure.dictionary.property-name.json",
        // module.exports
        "support.type.object.module",
