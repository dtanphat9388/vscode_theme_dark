        "markup.heading.markdown",
        "meta.separator.markdown",
      ],
      "settings": {
        "foreground": "#11b9ac"
      }
    },

    {
      "name": "--11b9ac bold",
      "scope": [
        /* REGEX */
        "constant.other.character-class.regexp",
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#11b9ac"
      }
    },

    
    //
    // TODO  E06C75
    //


    {
      "name": "--e06c75",
      "scope": [
        "keyword",
        // Language Variable
        "variable.language",
        // Built-in constant
        "constant.language",
        // Tag name
        "entity.name.tag.html",
        "entity.name.tag.class.js",
        "entity.name.tag.class.jsx",
        "support.class.component.js",
        // number
        "constant.numeric",
        /* JS | TS */
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        /* NOTE  MARKDOWN */
        "markup.fenced_code.block.markdown",
        /* TS */
        "punctuation.separator.key-value",
        /* REGEX */
        "keyword.control.anchor.regexp",
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },

    
    //
    // TODO  EBD996
    //


    {
      "name": "--EBD996",
      "scope": [
        /* JS */
        "string.quoted.double.js",
        "string.quoted.single.js",
        "string.template.js",
        "meta.object-literal.key", // object key
        // CSS
        "meta.selector.css",
        "constant.other.character-class.range.regexp",
        // TS
        "string.quoted.double.ts",
        "string.quoted.single.ts",
        "string.template.ts",
        "variable.other.enummember.ts",
        /* REGEX */
        "string.regexp",
        "meta.group.regexp",
        "constant.character.escape.backslash.regexp",
        /* HTML */
        "meta.attribute",
        /* JSON */
        "string.quoted.double.json",
        /* DOCKER */
        "string.quoted.double.dockerfile",
        /* MARKDOWN */
        "markup.quote.markdown",
      ],
      "settings": {
        "foreground": "#EBD996"
      }
    },

    
    //
    // TODO  62BAE4
    //


    {
      "name": "--62BAE4",
      "scope": [         
       /*  if, else, for, while ... */
        "keyword.control.conditional",  
        "keyword.control.loop",  
        /* for, while, ... */
        "keyword.control.flow",         
        /* function call */
        "meta.function-call",     
        /* Variable */
        "variable.readwrite",
        "variable.readwrite.other.block",
        // class super function
        "variable.language.super",
        "new.expr",
        /* HTML */
        "meta.attribute.event-handler.click.html",
        /* MARKDOWN */
        "markup.underline.link",
        /* TS */
        "meta.type.annotation",
        "meta.return.type",
      ],
      "settings": {
        "foreground": "#5ab8e4"
      }
    },

    {
      "name": "--5ab8e4 bold",
      "scope": [
        /* Regex */
        "punctuation.definition.character-class.regexp",
        "punctuation.definition.group.regexp",
      ],   
     "settings": {
       "fontStyle": "bold",
        "foreground": "#5ab8e4"
      }
    },

    
