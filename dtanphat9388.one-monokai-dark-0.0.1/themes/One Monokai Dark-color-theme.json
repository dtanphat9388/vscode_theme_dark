
{
  "tokenColors": [
    {
      "settings": {
        "background": "#282c34",
        "foreground": "#abb2bf",
        "findHighlight": "#FFE792",
        "findHighlightForeground": "#000000",
        "selectionBorder": "#222218",
        "activeGuide": "#9D550FB0",
        "bracketsForeground": "#F8F8F2A5",
        "bracketsOptions": "underline",
        "bracketContentsForeground": "#F8F8F2A5",
        "bracketContentsOptions": "underline",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#676f7d",
        "fontStyle": "italic"
      }
    },

    //
    // MARK  DATA TYPE
    //

    // NOTE  STRING
    {
      "name": "#e5c07b",
      "scope": [
        "string",
        "string.template.js",
        "meta.object-literal.key.js" // object key
      ],
      "settings": {
        "foreground": "#EAD87F"
        // "foreground": "#97E59B"
      }
    },
  
    {
      "name": "#c678dd",
      "scope": [
        // number
        "constant.numeric", 
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },

    {
      "name": "#bbbbbbff",
      "scope": [
        // computed object key
        "meta.brace.square.js",               
        // Object key value separator
        "punctuation.separator.key-value.js", 
        // declare function name
        "meta.definition.function",           
        // arrow function sign (ex: =>)
        "storage.type.function.arrow.js",     
        // User defined
        "meta.definition.variable",
        "variable.other.readwrite",
        // template string
        "meta.template.expression.js", 
        // js template-expression
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        // class name
        "entity.name.class",
        "entity.name.module",
        "storage.identifier",
        // inherited class
        "entity.other.inherited-class",
        // tag, component attribute name
        "entity.other.attribute-name",
        // lib class/type
        "support.type",
        // Json Property
        "support.dictionary.json"
      ],
      "settings": {
        "foreground": "#bbbbbbff"
      }
    },

    {
      "name": "#62BAE4",
      "scope": [
        // function call (ex: ())
        "meta.brace.round",             
        // if, else, ...
        "keyword.control.conditional",  
        // for, while, ...
        "keyword.control.flow",         
        // function call
        "meta.function-call",     
        // Variable
        "variable.readwrite",
        "variable.readwrite.other.block",
        // class super function
        "variable.language.super",
        "new.expr"      
      ],
      "settings": {
        "foreground": "#62BAE4"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d19a66"
      }
    },

    {
      "name": "Builtin Functions",
      "scope": [
        "function.support.builtin",
        "function.support.core"
      ],
      "settings": {
        "foreground": "#abb2bf"
      }
    },

    // MARK 
    
    {
      "name": "Embeded String Begin and End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Embeded String",
      "scope": "string.embedded",
      "settings": {
        "foreground": "#98c379"
      }
    },

    {
      "name": "#e06c75",
      "scope": [
        "keyword",
        // Language Variable
        "variable.language",
        // Built-in constant
        "constant.language",
        // Tag name
        "entity.name.tag",
        "entity.name.tag.class.js",
        "entity.name.tag.class.jsx",
        "support.class.component.js",
        // number
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "#56b6c2",
      "scope": [
        // User-defined constant
        "constant.character",
        "constant.other",
        // Storage
        "storage",
        "keyword.control",
        // Tag Class and ID
        "entity.name.tag.class",
        "entity.name.tag.id",
        // Library constant
        "support.constant",
        // StyleSheet Variable
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass",
        // StyleSheet Function
        "function.css",
        "function.scss",
        "function.less",
        "function.sass",
        // JSON String
        "structure.dictionary.property-name.json"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },

    
    
    {
      "name": "#abb2bf",
      "scope": [
        // Other variable
        "variable.other.property",
        "variable.other.block",
        // StyleSheet Property name
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#abb2bf"
      }
    },

    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },

    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "StyleSheet Unit",
      "scope": [
        "unit.css",
        "unit.scss",
        "unit.less",
        "unit.sass"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },


    // MARK 

    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#c678dd",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#56b6c2",
        "foreground": "#F8F8F0"
      }
    },

    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#56b6c2A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "none",
        "foreground": "#696969"
      }
    },

    // MARK  MARKDOWN

    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "Markdown: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Bold Punctuation",
      "scope": "punctuation.definition.bold.markdown",
      "settings": {
        "fontStyle": "none",
        "foreground": "#696969"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "fontStyle": "none",
        "foreground": "#696969"
      }
    },
    {
      "name": "Markdown: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Markdown: Raw",
      "scope": "markup.raw.inline.markdown, markup.raw.block.markdown",
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Markdown: List Punctuation",
      "scope": "punctuation.definition.list_item.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    }
  ],
  
